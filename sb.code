; salvobal!
speler: boog(pos, 8%)

L = speler
	pos := (5, 0)
	kleur := geel

R = speler
	pos := (0, 15)
	kleur := blauw

bal: cirkel
	straal := 5%
	kleur := groen

; invoer
als toetsLinks dan Δ L.pos = -1
als toetsRechts dan Δ L.pos = +1
als toetsOmhoog dan Δ L.pos = -1
als toetsOmlaag dan Δ L.pos = +1



uit = canvas(obj)
obj := {L, R, bal}
