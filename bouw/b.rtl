start:
	d := data (20)
	a := 123456789
	len := call (atoi, a, d)
	write(1, d, len)
	exit(0)
	eind

atoi:
	i := arg(0)
	str := arg(1)
	len := 0

	z := 0
	b := (i = z)
	ga (b, nul, lus)

nul:
	str(0) := '0'
	len += 1
	ret := len
	eind

lus:
	tien := 10
	(i,d) /= tien
	z := 0
	y := (i = z)
	ga (y, klaar, schrijf)

schrijf:
	e := '0'
	e += d
	str(len) := e
	len += 1
	ga lus

klaar:
	ret := len
	eind

