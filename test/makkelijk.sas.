1 = 1
true = true

3 + 2 + 1 = 6
1 + 2 * 3 = 7

100 _ 10 = 2
10 ^ 2 = 100
6 % 3 = 0
7 % 3 = 1
-1 % 3 = 2
sin 0 = 0
_^2 = 2
--3 = 3
2 - 2 = 0
2 + 2 = 4

; tekst
'hoi'.0 = 'h'
'hoi'.[1..3] = 'oi'
#'hoi' = 3
'ho' || 'oi' = 'hooi'
'a' || 'b' || 'c' = 'abc'

; lijst
0..3 = [0,1,2]
#[1,2] = 2
[1,2].0 = 1
[1,2,3].[0..2] = [1,2]
[1,2,3].[1..3] = [2,3]
;[1,2,3].[1..2] = [2]
;[1,2,3,4].[0..0] = none

(true xor false) = true
(false xor false) = false

(false => 2) = false
((true => 2) = 2) = true

; groepen
2.5 : int = false
2.0 : int = true

