gastheer = tcp.bind (0, 1237)
klanten = tcp.accepteer gastheer

uit = klanten zip1 stroom map tcp.schrijf

data = tekst(looptijd)

; http
stroom = kop ‖ h1 ‖ h2 ‖ h3 ‖ h4 ‖ data
kop = "HTTP/1.0 200 OK\r\n"
h1 = "Host: localhost\r\n"
h2 = "Server: Metamine\r\n"
h3 = "Content-Length: " ‖ tekst(#data) ‖ "\r\n"
h4 = "\r\n"
