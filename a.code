; beweegbare cirkel (gebruik pijltjestoetsen)
x := 10
y := 0

als toetsRechts en x' < 10 dan x := x' + 0.2 eind
als toetsLinks en x' > 0 dan x := x' - 0.2 eind
uit = canvas [ cirkel(x,y,1) ]

canvas = a → a

als toetsOmhoog en y' = 0 dan
	sprongStart := looptijd
	y := 10
eind

delta = looptijd - sprongStart
yvel = delta · -0.01

als y' > 0 dan
	y := y' - 0.1
eind
